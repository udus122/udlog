{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "next dev",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/next",
      "runtimeArgs": [
        "dev",
        "--turbo"
      ],
      "cwd": "${workspaceFolder}",
      "smartStep": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_OPTIONS": "--inspect"
      }
    },
    {
      "type": "node",
      "name": "vscode-jest-tests.v2",
      "request": "launch",
      "args": [
        "test",
        "--",
        "--runInBand",
        "--watchAll=false",
        "--testNamePattern",
        "${jest.testNamePattern}",
        "--runTestsByPath",
        "${jest.testFile}"
      ],
      "cwd": "/Users/yusuda/ghq/github.com/udus122/udlog",
      "internalConsoleOptions": "neverOpen",
    },
    {
      "name": "Debug TypeScript File with ts-node",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "cwd": "${workspaceRoot}",
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "args": [
        "--loader",
        "ts-node/esm",
        "${file}"
      ]
    }
  ]
}